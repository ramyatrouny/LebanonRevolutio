    "express": "~4.16.1",
    "express-rate-limit": "^5.0.0",
    "express-validator": "^6.2.0",
    "helmet": "^3.21.1",
    "http-status-codes": "^1.3.2",
    "moment": "^2.29.2",
    "mongoose": "^5.7.5",
    "morgan": "~1.9.1",
    "nodemon": "^1.19.4",
    "request-ip": "^2.1.3"
  }
}
